<odoo>
     <!--form view-->

     <report
     id="print_book"
     model="library.book"
     string="Imprimir book"
     report_type="qweb-pdf"
     name="facturacion_electronica.template_orden_book"
     />

    <template id="template_orden_book">
        <t t-foreach="docs" t-as="doc">
            <div class="article">
                <div class="col-xs-12">

                    <span t-field="doc.name"/>

                </div>

                <div class="row">
                    <div class="col-xs-4">
                     <strong>cols1</strong>
                    </div>
                    <div class="col-xs-4">
                       <strong>cols2</strong>
                    </div>
                    <div class="col-xs-4">
                        <strong>cols3</strong>
                    </div>





                </div>
            </div>
        </t>
    </template>
</odoo>